{
	"name": "@aknakos/sveltekit-danfojs-table",
	"version": "0.0.1",
	"homepage": "https://github.com/aknakos/sveltekit-danfojs-table#readme",
	"repository": "github:aknakos/sveltekit-danfojs-table",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"danfojs",
		"table"
	],
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"pp": "pnpm package && pnpm publish ./package --no-git-checks --access public"
	},
	"exports": {
		".": "./index.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.52",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.4.12",
		"tailwindcss": "^3.0.23",
		"daisyui": "^2.17.0",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"dependencies": {
		"@aknakos/sveltekit-danfojs": "^0.0.1"
	}
}
